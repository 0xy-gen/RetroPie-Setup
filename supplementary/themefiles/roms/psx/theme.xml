<theme>
	<listPrimaryColor>222222</listPrimaryColor>
	<descColor>222222</descColor>
	<listSelectorColor>aaaaaa</listSelectorColor>
	<hideHeader />
	<hideDividers />
	<listLeftAlign />

        <menuScrollSound>~/.emulationstation/themes/sounds/GS25.wav</menuScrollSound>
        <menuSelectSound>~/.emulationstation/themes/sounds/GS42.wav</menuSelectSound>
        <menuBackSound>~/.emulationstation/themes/sounds/GS28.wav</menuBackSound>
        <menuOpenSound>~/.emulationstation/themes/sounds/SNES5.wav</menuOpenSound>

        <component>
                <type>image</type>
                <path>~/.emulationstation/themes/psx_art/psx_bg.jpg</path>
                <pos>0 0</pos>
                <dim>1 1</dim>
		<origin>0 0</origin>
		<tiled />
        </component>

        <component>
                <type>image</type>
                <path>~/.emulationstation/themes/psx_art/psx_divider.png</path>
                <pos>.5 .5</pos>
                <dim>.01 1</dim>
		<origin>.5 .5</origin>
		<useAlpha />
        </component>

        <component>
                <type>image</type>
                <path>~/.emulationstation/themes/psx_art/psx_logo.png</path>
                <pos>.25 0</pos>
                <dim>0 .18</dim>
		<origin>.5 0</origin>
		<useAlpha />
        </component>

</theme>