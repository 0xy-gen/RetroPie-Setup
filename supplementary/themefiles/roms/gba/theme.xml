<theme>
	<listPrimaryColor>333333</listPrimaryColor>
	<descColor>000000</descColor>
	<listSelectorColor>FFFFFF</listSelectorColor>
	<hideHeader />
	<hideDividers />
	<listLeftAlign />

        <menuScrollSound>~/.emulationstation/themes/sounds/GS25.wav</menuScrollSound>
        <menuSelectSound>~/.emulationstation/themes/sounds/GS42.wav</menuSelectSound>
        <menuBackSound>~/.emulationstation/themes/sounds/GS28.wav</menuBackSound>
        <menuOpenSound>~/.emulationstation/themes/sounds/SNES5.wav</menuOpenSound>

        <component>
                <type>image</type>
                <path>~/.emulationstation/themes/gba_art/gba_bg_stripes.png</path>
                <pos>0 0</pos>
                <dim>1 1</dim>
		<tiled />
        </component>

        <component>
                <type>image</type>
                <path>~/.emulationstation/themes/gba_art/shadow.png</path>
                <pos>1 0</pos>
                <dim>.5 1</dim>
		<origin>1 0</origin>
        </component>

        <component>
                <type>image</type>
                <path>~/.emulationstation/themes/gba_art/gba_bg_left.jpg</path>
                <pos>.25 0</pos>
                <dim>.5 1</dim>
		<origin>.5 0</origin>
        </component>

        <component>
                <type>image</type>
                <path>~/.emulationstation/themes/gba_art/gba_logo.png</path>
                <pos>.25 0</pos>
                <dim>.4 0</dim>
		<origin>.5 0</origin>
        </component>
</theme>